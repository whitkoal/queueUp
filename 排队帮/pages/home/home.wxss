/* pages/home/home.wxss */
page{
  background-color: #ffffff;
}
.homebtn1{  
  margin-top: 60%;
  display: inline-block;
  padding:1px 30px;
  font-size:20px;
  cursor:pointer;
  text-decoration: none;
  outline:none;
  color:#e8ecc1;
  background-color:#131212;
  border-radius: 15px;
  box-shadow:0 9px #999;
}
.homebtn1:hover{
  background-color:#131212;
}
.homebtn1:active{
  background-color: #131212;
  box-shadow: 0 5px #666;
  transform: translateY(4px);
}




