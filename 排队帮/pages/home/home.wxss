/* pages/home/home.wxss */
page{
  background-color: #f5f5f5;
}

.paiduiinq{
  display: flex;
  align-content: center;
  justify-content: center;
  margin-top:10px; 
  width: 100%;
  height: 100%;
}
.paiduiing{
  background-color: #ffffff;
  width:85%;
  height: 200rpx;
  justify-content: center;
  display: -webkit-flex;
  align-items: center;
  border-radius: 25px;
  box-shadow: 10px 12px 14px #535353;
  font-size: 25px;
}
.guanliinq{
  display: flex;
