/* pages/home/home.wxss */
page{
  background-color: #f3f3f3;
}
.homebtn1{
  margin-top: 300px;
  margin-left: 120px;
  display: inline-block;
  padding:1px 25px;
  font-size:24px;
  cursor:pointer;
  text-decoration: none;
  outline:none;
  color:#f2ff7b;
  background-color:#131212;
  border-radius: 15px;
  box-shadow:0 9px #999;
}
.homebtn1:hover{
  background-color:#131212;
}
.homebtn1:active{
  background-color: #131212;
  box-shadow: 0 5px #666;
  transform: translateY(4px);
}




