.one{
  display: flex;
  align-content: center;
  justify-content: center;
  margin-top:400px; 
  width: 100%;
  height: 100%;
}
.homebtn1{
  padding:0px 25px;
  font-size:24px;
  cursor:pointer;
  text-decoration: none;
  outline:none;
  color:rgb(240, 248, 163);
  background-color:#000000;
  border-radius: 50px;
  box-shadow:0 9px #999;
}
.homebtn1:hover{
  background-color:#000000
}
.homebtn1:active{
  background-color: #000000;
  box-shadow: 0 5px #666;
  transform: translateY(4px);
}



